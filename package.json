{
  "name": "fake-account-detector",
  "version": "1.0.0",
  "description": "A web application that analyzes social media profiles to determine if they are real or fake",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "server-install": "npm install",
    "install-all": "npm run server-install && npm run client-install",
    "dev-all": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": [
    "fake account",
    "social media",
    "detection",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "puppeteer": "^21.5.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}